<xml xmlns="https://developers.google.com/blockly/xml">
  <category name="Logic" categorystyle="logic_blocks">
    <block type="controls_if"></block>
    <block type="logic_compare">
      <field name="OP">EQ</field>
    </block>
    <block type="logic_operation">
      <field name="OP">AND</field>
    </block>
    <block type="logic_negate"></block>
    <block type="logic_boolean">
      <field name="BOOL">TRUE</field>
    </block>
    <block type="logic_ternary"></block>
  </category>
  <category name="Math" categorystyle="math_blocks">
    <block type="math_number">
      <field name="NUM">0</field>
    </block>
    <block type="unlisp_math_single">
      <field name="OP">NEG</field>
      <value name="NUM">
        <shadow type="math_number">
          <field name="NUM">9</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_math_number_property">
      <mutation divisor_input="false"></mutation>
      <field name="PROPERTY">EVEN</field>
      <value name="NUMBER_TO_CHECK">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="math_arithmetic">
      <field name="OP">ADD</field>
      <value name="A">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="B">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
    </block>
    <block type="math_modulo">
      <value name="DIVIDEND">
        <shadow type="math_number">
          <field name="NUM">64</field>
        </shadow>
      </value>
      <value name="DIVISOR">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
  </category>
  <category name="Loops" categorystyle="loop_blocks">
    <block type="unlisp_while_itr">
      <field name="ITR">iterator</field>
    </block>
    <block type="unlisp_controls_repeat">
      <value name="TIMES">
        <shadow type="math_number">
          <field name="NUM">10</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_controls_whileUntil">
      <field name="MODE">WHILE</field>
    </block>
  </category>
  <category name="Text" categorystyle="text_blocks">
    <block type="text">
      <field name="TEXT"></field>
    </block>
    <block type="unlisp_text_quote">
      <value name="VALUE">
        <shadow type="math_arithmetic">
          <field name="OP">ADD</field>
          <value name="A">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
          <value name="B">
            <shadow type="math_number">
              <field name="NUM">1</field>
            </shadow>
          </value>
        </shadow>
      </value>
    </block>
    <block type="unlisp_text_eval">
      <value name="QUOTE">
        <shadow type="unlisp_text_quote">
          <value name="VALUE">
            <shadow type="math_arithmetic">
              <field name="OP">ADD</field>
              <value name="A">
                <shadow type="math_number">
                  <field name="NUM">1</field>
                </shadow>
              </value>
              <value name="B">
                <shadow type="math_number">
                  <field name="NUM">1</field>
                </shadow>
              </value>
            </shadow>
          </value>
        </shadow>
      </value>
    </block>
    <block type="text_print">
      <value name="TEXT">
        <shadow type="text">
          <field name="TEXT">abc</field>
        </shadow>
      </value>
    </block>
  </category>
  <category name="Lists" categorystyle="list_blocks">
    <block type="lists_create_with">
      <mutation items="0"></mutation>
    </block>
    <block type="lists_create_with">
      <mutation items="3"></mutation>
    </block>
    <block type="unlisp_statement_list"></block>
    <block type="unlisp_list_car"></block>
    <block type="unlisp_list_cdr"></block>
  </category>
  <sep></sep>
  <category name="Variables" categorystyle="variable_blocks" custom="VARIABLE"></category>
  <category name="Functions" categorystyle="procedure_blocks" custom="PROCEDURE"></category>
  <sep></sep>
  <category name="Special" categorystyle="special_blocks">
    <block type="unlisp_special_task">
      <value name="TIMES">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="MS">
        <shadow type="math_number">
          <field name="NUM">500</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_special_task_pass">
      <field name="PASS">task pass</field>
    </block>
    <block type="unlisp_special_dwrite">
      <value name="ID">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="STATE">
        <shadow type="logic_boolean">
          <field name="BOOL">TRUE</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_special_dread">
      <value name="ID">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_special_awrite">
      <value name="ID">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
      <value name="STATE">
        <shadow type="math_number">
          <field name="NUM">123</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_special_aread">
      <value name="ID">
        <shadow type="math_number">
          <field name="NUM">0</field>
        </shadow>
      </value>
    </block>
    <block type="unlisp_special_primitive_value">
      <mutation items="2" is_return='true'></mutation>
    </block>
  </category>
</xml>